{
  "name": "github npm stats",
  "version": "1.3.4",
  "manifest_version": 2,
  "description": "Displays npm package stats on GitHub",
  "author": "Ahmet KatrancÄ± <https://github.com/katranci>",
  "icons": {
    "16": "icons/icon16.png",
    "48": "icons/icon48.png",
    "128": "icons/icon128.png"
  },
  "permissions": [
    "storage",
    "https://*.npmjs.org/"
  ],
  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": [
        "chart.min.js",
        "bundle.js"
      ],
      "css": ["styles.css"]
    }
  ],
  "options_ui": {
  "page": "options.html",
  "open_in_tab": false
  }
}
